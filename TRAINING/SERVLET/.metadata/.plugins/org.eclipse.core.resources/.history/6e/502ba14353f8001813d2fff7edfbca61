package com.jpm.otm.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="DEPT_OTM")
public class Department {
	
	@Id
	@Column(name="dept_id")
	private Long deptId;
	private String deptName;
	

}
