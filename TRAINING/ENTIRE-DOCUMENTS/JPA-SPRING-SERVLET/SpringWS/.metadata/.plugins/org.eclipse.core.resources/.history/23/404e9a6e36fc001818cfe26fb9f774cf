package com.java;

// Factory class.
public class ObjectFactory {
	private MyApplDao dao;
	private MyApplServices service;
	
	public ObjectFactory(){
		dao = new MyApplDao();
		service = new MyApplServices(dao);
	}
	
	// Factory method.
	public MyApplDao getDao(){
		return dao;
	}
	
	public MyApplSerivces getService(){
		return service;
	}
}
