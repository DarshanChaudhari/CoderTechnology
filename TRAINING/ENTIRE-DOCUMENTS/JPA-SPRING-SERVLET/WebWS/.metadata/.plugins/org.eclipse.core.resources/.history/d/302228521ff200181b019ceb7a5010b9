package com.jp.hr.entities;

public class Employee {
	private int empId;
	private String firstName;
	private String lastName;
	
	public Employee(int empId, String firstName, String lastName) {
		this.empId = empId;
		this.firstName = firstName;
		this.lastName = lastName;
	}

	public Employee() {
		super();
	}
	
	
}
